{
    "name": "carlos-meneses/laravel-mpdf",
    "description": "Laravel Mpdf: Using Mpdf in Laravel for generate Pdfs.",
    "keywords": ["mpdf", "pdf", "laravel"],
    "license": "MIT",
    "type": "library",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2.5",
        "mpdf/mpdf": "^8.0"
    },
    "require-dev": {
        "illuminate/support": "^7.0|^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Meneses\\LaravelMpdf\\": "src/LaravelMpdf"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meneses\\LaravelMpdf\\LaravelMpdfServiceProvider"
            ],
            "aliases": {
                "LaravelMpdf": "Meneses\\LaravelMpdf\\Facades\\LaravelMpdf"
            }
        }
    }
}
